@model IEnumerable<AppUser>

<h1>Регистрирани корисници</h1>

<table class="table table-sm table-bordered">
    <tr><th>ID</th><th>Име и презиме</th><th>Email</th></tr>
    @foreach (AppUser user in Model)
    {
        <tr>
            <td>@user.Id</td>
            <td>@user.UserName</td>
            <td>@user.Email</td>
        </tr>
    }
</table>